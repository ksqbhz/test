{  "appenders": {    "console": {      "type": "console"    },    "dateFile": {      "type": "dateFile",      "filename": "./log/access.log",      "pattern": "-yyyy-MM-dd",      "category": "http"    },    "file":{      "type": "dateFile",      "filename": "./log/app.log",      "maxLogSize": 10485760,      "numBackups": 3    },    "error":{      "type": "dateFile",      "filename": "./log/errors.log",      "maxLogSize": 10485760,      "numBackups": 3,      "level": "ERROR"    }  },  "replaceConsole":true,  "categories": {    "http": { "appenders": ["dateFile"], "level": "trace" },    "app":{ "appenders": ["file"], "level": "info" },    "errors":{ "appenders": ["error"], "level": "error" },    "default": { "appenders": ["console"], "level": "info" }  }}